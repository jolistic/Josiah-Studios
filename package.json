{
  "name": "gatsby-starter-portfolio-jodie",
  "private": true,
  "description": "Image-heavy photography portfolio with colorful accents & great typography",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env CYPRESS_SUPPORT=y gatsby develop",
    "dev": "gatsby develop -o",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write content/**/*.yaml",
    "type-check": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "server:open": "start-server-and-test develop http://localhost:8000 cy:open",
    "server:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "server:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci",
    "test:compile": "tsc --project cypress/tsconfig.json",
    "test:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "test": "npm-run-all -s test:compile server:run",
    "test:watch": "npm-run-all -p test:compile:watch server:open",
    "test:ci": "npm-run-all -s test:compile server:ci"
  },
  "dependencies": {
    "@types/react-pdf": "^4.0.6",
    "babel-plugin-styled-components": "^1.11.1",
    "create-react-app": "^4.0.0",
    "gatsby": "^2.24.91",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-manifest": "^2.4.37",
    "gatsby-plugin-netlify": "^2.3.25",
    "gatsby-plugin-offline": "^3.2.38",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-typescript": "^2.4.25",
    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-source-instagram": "^0.8.0",
    "gatsby-transformer-sharp": "^2.5.20",
    "gatsby-transformer-yaml": "^2.4.15",
    "polished": "^3.6.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-pdf": "^5.0.0",
    "react-player": "^2.6.2",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "typeface-work-sans": "^0.0.72",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^10.17.44",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^5.0.16",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@types/testing-library__cypress": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.11.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-cypress": "^0.4.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.5"
  },
  "resolutions": {
    "cypress": "^4.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LekoArts/gatsby-starter-portfolio-jodie"
  },
  "bugs": {
    "url": "https://github.com/LekoArts/gatsby-starter-portfolio-jodie/issues"
  }
}
